{
    "name": "envey",
    "version": "2.0.0",
    "description": "Type-safe configuration schemas for Node.js",
    "keywords": [
        "configuration",
        "config",
        "schema",
        "validation",
        "zod",
        "env",
        "environment",
        "node",
        "nodejs"
    ],
    "homepage": "https://github.com/samialdury/envey#readme",
    "bugs": {
        "url": "https://github.com/samialdury/envey/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/samialdury/envey.git"
    },
    "license": "MIT",
    "author": "Sami Al-Dury",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "./dist",
        "./src"
    ],
    "devDependencies": {
        "@commitlint/cli": "17.7.1",
        "@samialdury/config": "5.6.1",
        "@types/node": "20.5.6",
        "@vitest/coverage-v8": "0.34.2",
        "eslint": "8.47.0",
        "husky": "8.0.3",
        "lint-staged": "14.0.1",
        "prettier": "3.0.2",
        "semantic-release": "21.1.1",
        "tsup": "7.2.0",
        "typescript": "5.2.2",
        "vitest": "0.34.2",
        "zod": "3.22.2"
    },
    "peerDependencies": {
        "zod": "^3.0.0"
    },
    "engines": {
        "node": ">=18"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
