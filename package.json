{
    "name": "envey",
    "version": "2.2.1",
    "description": "Type-safe configuration schemas for Node.js",
    "keywords": [
        "configuration",
        "config",
        "schema",
        "validation",
        "zod",
        "env",
        "environment",
        "node",
        "nodejs"
    ],
    "homepage": "https://github.com/samialdury/envey#readme",
    "bugs": {
        "url": "https://github.com/samialdury/envey/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/samialdury/envey.git"
    },
    "license": "MIT",
    "author": "Sami Al-Dury",
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "./dist",
        "./src"
    ],
    "devDependencies": {
        "@commitlint/cli": "18.4.3",
        "@samialdury/config": "6.2.0",
        "@types/node": "20.9.5",
        "@vitest/coverage-v8": "0.34.6",
        "eslint": "8.54.0",
        "husky": "8.0.3",
        "lint-staged": "15.1.0",
        "prettier": "3.1.0",
        "semantic-release": "22.0.8",
        "tsup": "8.0.1",
        "typescript": "5.3.2",
        "vitest": "0.34.6",
        "zod": "3.22.4"
    },
    "peerDependencies": {
        "zod": "^3.0.0"
    },
    "engines": {
        "node": ">=20"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
